{
	"name": "ijsblokje",
	"version": "1.0.0",
	"description": "A personal GitHub bot which syncs data and runs automated systems",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"homepage": "https://github.com/ijsKoud/ijsblokje",
	"keywords": [
		"probot",
		"github",
		"probot-app"
	],
	"scripts": {
		"build": "tsc --build",
		"dev": "NODE_ENV=development nodemon -w src --ext ts,json --exec 'yarn build && yarn start'",
		"start": "NODE_ENV=production probot run ./dist",
		"lint": "eslint src",
		"postinstall": "is-ci || husky install",
		"test": "jest"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"probot": "^11.0.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-angular": "^17.0.0",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/jest": "^26.0.19",
		"@types/node": "^16.11.41",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"is-ci": "^3.0.1",
		"jest": "^26.6.3",
		"lint-staged": "^13.0.2",
		"nock": "^13.0.5",
		"prettier": "^2.7.1",
		"smee-client": "^1.2.3",
		"ts-jest": "^26.4.4",
		"typescript": "^4.7.4"
	},
	"engines": {
		"node": ">= 10.13.0"
	}
}
