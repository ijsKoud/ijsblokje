{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"outDir": "./dist",
		"noUnusedParameters": false,
		"useUnknownInCatchVariables": false,
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"skipLibCheck": true
	},
	"buildOptions": {
		"incremental": true,
		"dry": true
	},
	"exclude": ["node_modules"]
}
